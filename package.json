{
  "name": "nubook",
  "description": "The NUbots team handbook.",
  "version": "1.0.0",
  "author": "NUbots",
  "private": true,
  "dependencies": {
    "@mdx-js/mdx": "^1.1.5",
    "@mdx-js/react": "^1.1.5",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "focus-visible": "^5.0.2",
    "gatsby": "^2.13.50",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-mdx": "^1.0.0",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-postcss": "^2.1.2",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-remark-autolink-headers": "^2.1.4",
    "gatsby-remark-copy-linked-files": "^2.1.5",
    "gatsby-remark-images": "^3.1.8",
    "gatsby-remark-katex": "^3.1.2",
    "gatsby-source-filesystem": "^2.1.8",
    "gatsby-transformer-remark": "^2.6.14",
    "gatsby-transformer-sharp": "^2.2.5",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.4",
    "katex": "^0.11.0",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.18.2",
    "prism-react-renderer": "^0.1.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-transition-group": "^4.2.2",
    "remark-unwrap-images": "^1.0.0",
    "rimraf": "^3.0.0",
    "tailwindcss": "^1.1.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "clean": "rimraf .cache/ public/",
    "lint": "eslint \"src/**/*.{jsx,js}\" \"*.js\"",
    "format": "prettier --write \"src/**/*.{css,jsx,js,mdx}\" \"*.js\"",
    "format:check": "prettier --check \"src/**/*.{css,jsx,js,mdx}\" \"*.js\"",
    "deploy": "node scripts/verify-deploy-branch.js && npm run lint && npm run clean && gatsby build --prefix-paths && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NUbots/NUbook"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "jsxBracketSameLine": false,
    "endOfLine": "lf"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  }
}
